{
  "eslint.format.enable": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.fixAll.eslint": true
  },
  "stylelint.validate": [
    "css",
    "less",
    "postcss",
    "scss",
    "sass",
    "vue",
  ],
  "[vue]": {
    "gitlens.codeLens.scopes": [
      "document"
    ],
    "inlineFold.regex": ".*?=\"(.{20,})\"",
    "inlineFold.regexGroup": "1",
    "inlineFold.maskChar": "░░░",
    "inlineFold.unfoldOnLineSelect": true
  },
  "inlineFold.regex": ".*?=\\\"(.{20,})\\\"",
}